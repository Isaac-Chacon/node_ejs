<!DOCTYPE html>
<html lang="pt-br">
  <!-- Inclui o html que está no arquivo head.ejs -->
  <%- include('../partials/head'); %>
  <body class="container">
    <!-- Inclui o html que está no arquivo header.ejs -->
    <%- include('../partials/header'); %>
    <main>
      <div class="jumbotron">
        <h1>Node com EJS</h1>
        <p>Vamos aprender a trabalhar com arquivos EJS</p>
        <span><%= subtitle %></span>
      </div>
      <ul>
        <% qualitys.forEach(function(quality){ %>
        <li><strong><%= quality.title %></strong> <%= quality.message %></li>
        <% }) %>
      </ul>
    </main>
    <!-- Inclui o html que está no arquivo footer.ejs -->
    <%- include('../partials/footer'); %>
  </body>
</html>
